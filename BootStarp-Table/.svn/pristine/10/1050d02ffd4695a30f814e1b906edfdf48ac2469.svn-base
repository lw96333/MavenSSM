                <!-- col start -->
                <div class="bs-example" data-example-id="hoverable-table">
                    <div class="card-box beijing"style="padding-right: 200px;text-align: center;">
                        <h4 class="header-title m-t-0 m-b-30 ziti">个人用户信息注册</h4>
                        <form class="am-form am-form-horizontal" >
                            <div class="am-form-group shuru">
                                <label for="name" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">用户名</label>
                                <div class="am-u-sm-9">
                                    <input type="text" id="name" placeholder="设置你的用户名" maxlength="12">
                                </div>
                            </div>
                            <div class="am-form-group shuru">
                                <label for="password" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">密码</label>
                                <div class="am-u-sm-9">
                                    <input type="password" id="password" placeholder="设置你的密码" maxlength="18">
                                </div>
                            </div>
                            <div class="am-form-group shuru">
                                <label for="realname" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">真实姓名</label>
                                <div class="am-u-sm-9">
                                    <input type="text" id="realname" placeholder="输入你的真实姓名" maxlength="5">
                                </div>
                            </div>
                            <div class="am-form-group shuru">
                                <label for="address" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">家庭地址</label>
                                <div class="am-u-sm-9">
                                    <input type="text" id="address" placeholder="输入你的家庭地址" maxlength="40">
                                </div>
                            </div>
                            <div class="am-form-group shuru">
                                <label for="phone" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">电话号码</label>
                                <div class="am-u-sm-9">
                                    <input type="text" id="phone" placeholder="输入你的电话号码" maxlength="11">
                                </div>
                            </div>
                            <div class="am-form-group shuru">
                                <label for="identitycard" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">身份证号码</label>
                                <div class="am-u-sm-9">
                                    <input type="text" id="identitycard" placeholder="输入你的身份证号码" maxlength="18">
                                </div>
                            </div>
                            <div class="am-form-group shuru">
                                <label for="professional" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">职业描述</label>
                                <div class="am-u-sm-9">
                                    <input type="text" id="professional" placeholder="输入你的职业描述" maxlength="30">
                                </div>
                            </div>
                            <div class="am-form-group shuru">
                                <label for="email" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">邮箱地址</label>
                                <div class="am-u-sm-9">
                                    <input type="email" id="email" placeholder="输入你的邮箱地址" maxlength="40">
                                </div>
                            </div>
 								<div class="am-u-sm-10 am-u-sm-offset-3" style="padding-left: 400px"><br/><br/>
