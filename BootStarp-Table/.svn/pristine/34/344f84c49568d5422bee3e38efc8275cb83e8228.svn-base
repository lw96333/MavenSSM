		<!-- Start right Content here -->
		<div class="content-page">
			<!-- Start content -->
			<div class="content">
				<!-- row start -->
				<div class="am-g">
					<!-- col start -->
					<div class="am-u-md-11">
						<div class="card-box">
							<h4 class="header-title m-t-0 m-b-30">修改登陆密码</h4>
							<form class="am-form am-form-horizontal" >
								<div class="am-form-group">
									<label for="" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">企业登陆名</label>
									<div class="am-u-sm-9">
										<input type="text" value=<shiro:principal property="loginname"/> disabled/>
									</div>
								</div>

								<div class="am-form-group">
									<label for="" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">公司名称</label>
									<div class="am-u-sm-9">
										<input type="text" value=<shiro:principal property="companyname"/> disabled/>
									</div>
								</div>
								<div class="am-form-group">
									<label for="" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">公司楼层</label>
									<div class="am-u-sm-9">
										<input type="text" value=<shiro:principal property="companyaddress"/> disabled/>
									</div>
								</div>
								<div class="am-form-group">
									<label for="" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">原登陆密码</label>
									<div class="am-u-sm-9">
										<input type="password" id="oldpw" maxlength="12"value="" />
									</div>
								</div>
								<div class="am-form-group">
									<label for="" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">新登陆密码</label>
									<div class="am-u-sm-9">
										<input type="password" id="newpw1" maxlength="12"value="" />
									</div>
								</div>
								<div class="am-form-group">
									<label for="" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0">确认登陆密码</label>
									<div class="am-u-sm-9">
										<input type="password" id="newpw2" maxlength="12"value="" />
									</div>
								</div>
								
								<div class="am-form-group" id="showerror">
									<label style="color:red;" class="am-u-sm-3 am-form-label am-text-right am-padding-right-0"></label>						
									<div class="am-u-sm-9"><br/>
										<label style="color:red;">您输入的内容有误，请修改后再提交！</label>
									</div>
									
								</div>



								<div class="am-form-group">
										<input id="companyid" value=<shiro:principal property='id'/> hidden/>
									<div class="am-u-sm-10 am-u-sm-offset-3" style="padding-left: 200px"><br/><br/>
										<a href="javascript:void(0)"> <button type="button" id="surechange" class="btn btn-info" style="width: 300px">确认修改</button>
										</a>
									</div>
								</div>
							</form>
						</div>
					</div>
					<!-- col end -->
				</div>
				<!-- row end -->
			</div>
				</div>



			</div>

		<!-- end right Content here -->
