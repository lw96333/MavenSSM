<body>
		<div id="container">
			<div id="box_center">
				前台/后台
				<select name="fangyuanEntity.fyXqCode" id="userType" class="ui_select01">
					<option value="">--请选择--</option>
					<option value="前台">前台</option>
					<option value="后台">后台</option>
				</select>

				操作/登录
				<select id="logType" class="ui_select01">
					<option value="">--请选择--</option>
					<option value="操作">操作</option>
					<option value="登录">登录</option>
				</select>
				日期
				<input type="text" class="form-control" id="date" name="brithday" readonly="readonly"
					    placeholder="请选择日期" onClick="WdatePicker()" style="height: 30px;width: 150px;display: inline-block">

				用户名
				<input type="text" id="userName" style="height: 30px">
				
			</div>
			<div class="ui_content">
				<div class="ui_tb">
				
					<table class="table table-hover" id="table_log"></table>
				
					
				</div>
				
				<br/>
			</div>

		</div>
