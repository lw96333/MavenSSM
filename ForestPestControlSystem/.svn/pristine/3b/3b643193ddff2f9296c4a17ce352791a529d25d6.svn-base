package com.one.bean.systemInformation;

import java.io.Serializable;
/**
 * 用户管理
 * @author 49160
 *
 */
public class User_Management_Bean implements Serializable{

	private Integer userId;//用户编号
	
	private String userName;//用户名
	
	private String password;//用户密码
	
	private String name;//真实姓名

	private User_Privileges_Bean privileges;//用户权限id
	
	public User_Management_Bean() {
		
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public User_Privileges_Bean getPrivileges() {
		return privileges;
	}

	public void setPrivileges(User_Privileges_Bean privileges) {
		this.privileges = privileges;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "User_Management_Bean [userId=" + userId + ", userName=" + userName + ", password=" + password
				+ ", name=" + name + ", privileges=" + privileges + "]";
	}
	
}
