package com.one.bean;

import java.io.Serializable;

/**
 *  灾情阶段
 * @author 
 *
 */
public class DisasterStageBean implements Serializable{

	private Integer stageId; // 灾情阶段ID
	
	private String typeName; // 灾情类型

	public DisasterStageBean() {
		
	}
	
	public DisasterStageBean(Integer stageId, String typeName) {
		super();
		this.stageId = stageId;
		this.typeName = typeName;
	}

	public Integer getStageId() {
		return stageId;
	}

	public void setStageId(Integer stageId) {
		this.stageId = stageId;
	}

	public String getTypeName() {
		return typeName;
	}

	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}
	
	
}
